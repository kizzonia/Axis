<div id="<%= dom_id account %>">
  <p>
    <strong>Balance:</strong>
    <%= account.balance %>
  </p>

  <p>
    <strong>Slug:</strong>
    <%= account.slug %>
  </p>

  <p>
    <strong>Revenue:</strong>
    <%= account.revenue %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= account.status %>
  </p>

  <p>
    <strong>Verify:</strong>
    <%= account.verify %>
  </p>

  <p>
    <strong>User:</strong>
    <%= account.user_id %>
  </p>

</div>
