<div class="dashboard__sidebar">
  <div class="dashboard_sidebar_list">
    <div class="sidebar_list_item">
      <%= link_to accounts_path, class: "items-center -is-active" do %>
      <i class="flaticon-house mr15"></i>Dashboard<% end %>
    </div>
    <div class="sidebar_list_item ">
      <%= link_to products_path, class: "items-center" do %> <i class="flaticon-cash-on-delivery mr15"></i>Products <% end %>
    </div>
    <div class="sidebar_list_item ">
      <%= link_to orders_path, class: "items-center" do %> <i class="flaticon-cash-on-delivery mr15"></i>Orders<% end %>
    </div>
    <div class="sidebar_list_item ">
      <% @wallets.each do |a| %>
      <%= link_to a, class: "items-center" do %><i class="flaticon-growth mr15"></i>Wallet<% end %>
      <% end %>
    </div>


    <div class="sidebar_list_item ">
      <%= link_to transactions_path, class: "items-center" do %> <i class="flaticon-transaction mr15"></i>Orders<% end %>
    </div>
    <div class="sidebar_list_item ">
      <%= link_to edit_user_registration_path, class: "items-center" do %>
  <i class="flaticon-settings mr15"></i>Settings<% end %>
    </div>
    <div class="sidebar_list_item ">
      <%= link_to welcomes_path, class: "items-center" do %> <i class="flaticon-cash-on-delivery mr15"></i>Goto Shop <% end %>

      <!-- <a href="page-dashboard-categories.html" class="items-center"><i class="flaticon-folder mr15"></i>Categories</a> -->
    </div>
    <div class="sidebar_list_item ">
      <%= link_to  destroy_user_session_path, class: "items-center", method: :delete do %>

      <i class="flaticon-exit mr15"></i>Logout<% end %>
    </div>
  </div>
</div>
