  <!-- Top Category -->
  <% if About.exists? %>
  <section class="top-category pb30 pt0">
    <div class="container">
      <div class="row mt70">
        <div class="col-lg-12">
          <div class="banner_one home1_style before_none color1 home3_large_banner mb30">
            <!-- <div class="thumb style1"><img src="images/banner/banner1-home3.png" alt="Banner Img"></div> -->
            <% @headers.each do |a| %>
            <div class="details col-md-6 col-sm-12">
              <h3 class="title"><%= a.abouts_title %></h3>
              <p class="para "><strong><%= a.abouts_sub_title %></strong></p>
              <!-- <a href="page-shop-list-v1.html" class="shop_btn">Shop Now</a> -->
            </div>
            <% end %>
            <!-- <div class="col-md-6">

            </div> -->
          </div>
        </div>
      </div>
    </div>
  </section>
  <% else %>
  <section class="top-category pb30 pt0">
    <div class="container">
      <div class="row mt70">
        <div class="col-lg-12">
          <div class="banner_one home1_style before_none color1 home3_large_banner mb30">
            <!-- <div class="thumb style1"><img src="images/banner/banner1-home3.png" alt="Banner Img"></div> -->
            <div class="details col-md-6">
              <h3 class="title">Axis Marketplace</h3>
              <p class="para "><strong>At Axis Marketplace, we're dedicated to bringing you the best  products at prices that won't break the bank. We believe shopping should be enjoyable, easy, and rewarding.<br> That's why we've handpicked a
                diverse range of high-quality products that cater to every taste and need.</strong></p>
              <a href="page-shop-list-v1.html" class="shop_btn">Shop Now</a>
            </div>
            <div class="col-md-6">

            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <% end %>
