<div id="<%= dom_id feature %>">
  <p>
    <strong>Title:</strong>
    <%= feature.title %>
  </p>

  <p>
    <strong>Sub title:</strong>
    <%= feature.sub_title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= feature.body %>
  </p>

  <p>
    <strong>Slug:</strong>
    <%= feature.slug %>
  </p>

</div>
