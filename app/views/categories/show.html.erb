                <div class="row">

                  <% if @products.exists? %>
                  <% @products.each do |p| %>
                  <div class="col-md-6 col-lg-12">
                    <div class="shop_item list_style bdr1 my--1 align-items-center row">
                      <div class="thumb text-center text-md-start col-xl-4 col-xxl-3">
                        <img src="<%= p.prodimage_url.to_s %>" alt="Shop Item1">
                      </div>
                      <div class="details ps-0 ps-lg-4 col-xl-8 col-xxl-6">
                        <div class="sub_title"><%= p.name %></div>
                        <div class="title"><%= link_to p do %><%= p.name %> - <%= p.title %><% end %></div>
                        <div class="review d-block d-md-flex">
                          <ul class="m-0 mb-md-3 me-2">
