		url(' <%= asset_path ('fonts/flaticon60b2.woff2') %>') format('woff2'),
		url(' <%= asset_path ('fonts/flaticon60b2.woff') %>') format('woff'),
		url(' <%= asset_path ('fonts/flaticon60b2.ttf') %>') format('truetype'),
		url('<%= asset_path ('fonts/flaticon60b2.svg') %>') format('svg');
	font-weight: normal;
	font-style: normal;
};
